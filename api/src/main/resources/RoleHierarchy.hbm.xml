<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.intalio.tempo.security.database.model.RoleHierarchy" table="security_role_hierarchy" >
	    <composite-id>
	        <key-many-to-one name="role" class="org.intalio.tempo.security.database.model.Role">
	            <column name="role_id" />
	        </key-many-to-one>
	        <key-many-to-one name="descendantRole" class="org.intalio.tempo.security.database.model.Role">
	            <column name="descendant_role_id" />
	        </key-many-to-one>
	    </composite-id>
    </class>
</hibernate-mapping>
